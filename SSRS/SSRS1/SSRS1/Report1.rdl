  <DataSets>
    <DataSet Name="dsSalesOrderDetails">
      <Query>
        <DataSourceName>dsAdventureWorks</DataSourceName>
        <CommandText>SELECT        p.Name, sod.SalesOrderID, sod.SalesOrderDetailID, sod.UnitPrice
FROM            Sales.SalesOrderDetail AS sod INNER JOIN
                         Production.Product AS p ON sod.ProductID = p.ProductID
GROUP BY p.Name, sod.SalesOrderDetailID, sod.SalesOrderID, sod.UnitPrice</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesOrderID">
          <DataField>SalesOrderID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesOrderDetailID">
