  <local:Sum.Implementation>
    <StateMachine>
      <StateMachine.InitialState>
        <State x:Name="__ReferenceID1" DisplayName="StartMessage">
          <State.Entry>
            <WriteLine Text="Pleasae insert a number to sum or zero for total..." />
          </State.Entry>
          <State.Transitions>
            <Transition DisplayName="T1">
              <Transition.To>
                <State x:Name="__ReferenceID0" DisplayName="TalkingInput">
                  <State.Entry>
                    <Sequence>
                      <Sequence>
                        <WriteLine Text="number or zero? " />
                        <local:ReadInput>
                          <local:ReadInput.Result>
                            <OutArgument x:TypeArguments="x:Int32">
                              <mca:CSharpReference x:TypeArguments="x:Int32">number</mca:CSharpReference>
                            </OutArgument>
                          </local:ReadInput.Result>
                        </local:ReadInput>
                        <Assign>
                          <Assign.To>
                            <OutArgument x:TypeArguments="x:Int32">
                              <mca:CSharpReference x:TypeArguments="x:Int32">SumOfAll</mca:CSharpReference>
                            </OutArgument>
                          </Assign.To>
                          <Assign.Value>
                            <InArgument x:TypeArguments="x:Int32">
                              <mca:CSharpValue x:TypeArguments="x:Int32">number+SumOfAll</mca:CSharpValue>
                            </InArgument>
                          </Assign.Value>
                        </Assign>
                      </Sequence>
                    </Sequence>
                  </State.Entry>
                  <State.Transitions>
                    <Transition DisplayName="T2">
                      <Transition.Condition>
                        <mca:CSharpValue x:TypeArguments="x:Boolean">number == 0</mca:CSharpValue>
                      </Transition.Condition>
                      <Transition.To>
                        <State x:Name="__ReferenceID2" DisplayName="FinalState" IsFinal="True">
                          <State.Entry>
                            <WriteLine>
                              <InArgument x:TypeArguments="x:String">
                                <mca:CSharpValue x:TypeArguments="x:String">"Sum of all number are "+ SumOfAll</mca:CSharpValue>
                              </InArgument>
                            </WriteLine>
                          </State.Entry>
                        </State>
                      </Transition.To>
                    </Transition>
                    <Transition DisplayName="T3" To="{x:Reference __ReferenceID0}">
                      <Transition.Condition>
                        <mca:CSharpValue x:TypeArguments="x:Boolean">number != 0</mca:CSharpValue>
                      </Transition.Condition>
                    </Transition>
                  </State.Transitions>
                  <State.Variables>
                    <Variable x:TypeArguments="x:Int32" Name="number" />
                  </State.Variables>
                </State>
              </Transition.To>
            </Transition>
          </State.Transitions>
        </State>
